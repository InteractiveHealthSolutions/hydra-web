	<queryString>
		<![CDATA[select pt_id.identifier as patient_id, per_name.given_name as patient_first_name, per_name.family_name as patient_last_name,
per.birthdate as patient_birthdate, per.gender as patient_gender, loc.name as patient_location,
obs_alamat_saat_ini.value_text as alamat_saat_ini, desa_atau_kelurahan_concept.name as desa_atau_kelurahan,
kecamatan_concept.name as kecamatan, hm_wf.name as workflow, obs_kasus_indeks_id_pasien.value_text as kasus_indeks_id_pasien,
scr_int_enc.max_enc_datetime as screening_interview_datetime, batuk_lebih_dari_2_minggu_concept.name as batuk_lebih_dari_2_minggu,
batuk_kurang_dari_2_minggu_concept.name as batuk_kurang_dari_2_minggu, batuk_dara_concept.name as batuk_dara,
demam_concept.name as demam, obs_lamanya_demam.value_numeric as lamanya_demam, berat_badan_menurun_concept.name as berat_badan_menurun,
keringat_malam_concept.name as keringat_malam, gejala_tb_ekstrapulmonal_concept.name as gejala_tb_ekstrapulmonal,
status_hiv_concept.name as status_hiv, kesimpulan_pemeriksaan_uji_tuberculin_concept.name as kesimpulan_pemeriksaan_uji_tuberculin,
hasil_pembacaan_foto_rontgen_concept.name as hasil_pembacaan_foto_rontgen, kelainan_pada_paru_atau_pleura_concept.name as kelainan_pada_paru_atau_pleura,
mengumpulkan_dahak_concept.name as mengumpulkan_dahak, hasil_xpert_mtb_concept.name as hasil_xpert_mtb, hasil_xpert_rif_concept.name as hasil_xpert_rif
FROM ( SELECT patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type_id, encounter_id
		 FROM hydra.encounter
         INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'GeneXpert Details'
		 WHERE encounter.voided = 0
         GROUP BY encounter.patient_id, encounter.encounter_id) AS enc
inner join hydra.patient pt on pt.patient_id = enc.patient_id
inner join hydra.patient_identifier pt_id on pt.patient_id = pt_id.patient_id and pt_id.identifier_type = 3 and pt_id.voided = 0
left join hydra.location loc on pt_id.location_id = loc.location_id
left join hydra.person per on pt.patient_id = per.person_id
left join hydra.person_name per_name on per.person_id = per_name.person_id and per_name.voided = 0 and per_name.preferred = 1
left join hydra.person_attribute hlt_ctr on per.person_id = hlt_ctr.person_id and hlt_ctr.person_attribute_type_id = 7 and hlt_ctr.voided = 0
left join hydra.location hlt_ctr_name on hlt_ctr.value = hlt_ctr_name.location_id
LEFT JOIN hydra.obs obs_mengumpulkan_dahak ON enc.encounter_id = obs_mengumpulkan_dahak.encounter_id and obs_mengumpulkan_dahak.concept_id = 166966 and obs_mengumpulkan_dahak.voided = 0
LEFT JOIN hydra.concept_name mengumpulkan_dahak_concept ON  obs_mengumpulkan_dahak.value_coded = mengumpulkan_dahak_concept.concept_id and mengumpulkan_dahak_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_hasil_xpert_mtb ON enc.encounter_id = obs_hasil_xpert_mtb.encounter_id and obs_hasil_xpert_mtb.concept_id = 166971 and obs_hasil_xpert_mtb.voided = 0
LEFT JOIN hydra.concept_name hasil_xpert_mtb_concept ON  obs_hasil_xpert_mtb.value_coded = hasil_xpert_mtb_concept.concept_id and hasil_xpert_mtb_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_hasil_xpert_rif ON enc.encounter_id = obs_hasil_xpert_rif.encounter_id and obs_hasil_xpert_rif.concept_id = 166975 and obs_hasil_xpert_rif.voided = 0
LEFT JOIN hydra.concept_name hasil_xpert_rif_concept ON  obs_hasil_xpert_rif.value_coded = hasil_xpert_rif_concept.concept_id and hasil_xpert_rif_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Patient Registration'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) pt_reg_enc ON enc.patient_id = pt_reg_enc.patient_id
LEFT JOIN hydra.obs obs_alamat_saat_ini ON pt_reg_enc.encounter_id = obs_alamat_saat_ini.encounter_id and obs_alamat_saat_ini.concept_id = 166852 and obs_alamat_saat_ini.voided = 0
LEFT JOIN hydra.obs obs_desa_atau_kelurahan ON pt_reg_enc.encounter_id = obs_desa_atau_kelurahan.encounter_id and obs_desa_atau_kelurahan.concept_id = 166854 and obs_desa_atau_kelurahan.voided = 0
LEFT JOIN hydra.concept_name desa_atau_kelurahan_concept ON  obs_desa_atau_kelurahan.value_coded = desa_atau_kelurahan_concept.concept_id and desa_atau_kelurahan_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_kecamatan ON pt_reg_enc.encounter_id = obs_kecamatan.encounter_id and obs_kecamatan.concept_id = 166854 and obs_kecamatan.voided = 0
LEFT JOIN hydra.concept_name kecamatan_concept ON  obs_kecamatan.value_coded = kecamatan_concept.concept_id and kecamatan_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Index Case Details'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) idx_det_enc ON enc.patient_id = idx_det_enc.patient_id
LEFT JOIN hydra.obs obs_kasus_indeks_id_pasien ON idx_det_enc.encounter_id = obs_kasus_indeks_id_pasien.encounter_id and obs_kasus_indeks_id_pasien.concept_id = 166922 and obs_kasus_indeks_id_pasien.voided = 0
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Screening Interview'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) scr_int_enc ON enc.patient_id = scr_int_enc.patient_id
LEFT JOIN hydra.obs obs_batuk_lebih_dari_2_minggu ON scr_int_enc.encounter_id = obs_batuk_lebih_dari_2_minggu.encounter_id and obs_batuk_lebih_dari_2_minggu.concept_id = 166877 and obs_batuk_lebih_dari_2_minggu.voided = 0
LEFT JOIN hydra.concept_name batuk_lebih_dari_2_minggu_concept ON  obs_batuk_lebih_dari_2_minggu.value_coded = batuk_lebih_dari_2_minggu_concept.concept_id and batuk_lebih_dari_2_minggu_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_batuk_kurang_dari_2_minggu ON scr_int_enc.encounter_id = obs_batuk_kurang_dari_2_minggu.encounter_id and obs_batuk_kurang_dari_2_minggu.concept_id = 166878 and obs_batuk_kurang_dari_2_minggu.voided = 0
LEFT JOIN hydra.concept_name batuk_kurang_dari_2_minggu_concept ON  obs_batuk_kurang_dari_2_minggu.value_coded = batuk_kurang_dari_2_minggu_concept.concept_id and batuk_kurang_dari_2_minggu_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_batuk_dara ON scr_int_enc.encounter_id = obs_batuk_dara.encounter_id and obs_batuk_dara.concept_id = 166880 and obs_batuk_dara.voided = 0
LEFT JOIN hydra.concept_name batuk_dara_concept ON  obs_batuk_dara.value_coded = batuk_dara_concept.concept_id and batuk_dara_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_demam ON scr_int_enc.encounter_id = obs_demam.encounter_id and obs_demam.concept_id = 166881 and obs_demam.voided = 0
LEFT JOIN hydra.concept_name demam_concept ON  obs_demam.value_coded = demam_concept.concept_id and demam_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_lamanya_demam ON scr_int_enc.encounter_id = obs_lamanya_demam.encounter_id and obs_lamanya_demam.concept_id = 166882 and obs_lamanya_demam.voided = 0
LEFT JOIN hydra.obs obs_berat_badan_menurun ON scr_int_enc.encounter_id = obs_berat_badan_menurun.encounter_id and obs_berat_badan_menurun.concept_id = 166883 and obs_berat_badan_menurun.voided = 0
LEFT JOIN hydra.concept_name berat_badan_menurun_concept ON  obs_berat_badan_menurun.value_coded = berat_badan_menurun_concept.concept_id and berat_badan_menurun_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_keringat_malam ON scr_int_enc.encounter_id = obs_keringat_malam.encounter_id and obs_keringat_malam.concept_id = 166884 and obs_keringat_malam.voided = 0
LEFT JOIN hydra.concept_name keringat_malam_concept ON  obs_berat_badan_menurun.value_coded = keringat_malam_concept.concept_id   and keringat_malam_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_gejala_tb_ekstrapulmonal ON scr_int_enc.encounter_id = obs_gejala_tb_ekstrapulmonal.encounter_id and obs_gejala_tb_ekstrapulmonal.concept_id = 166884 and obs_gejala_tb_ekstrapulmonal.voided = 0
LEFT JOIN hydra.concept_name gejala_tb_ekstrapulmonal_concept ON  obs_gejala_tb_ekstrapulmonal.value_coded = gejala_tb_ekstrapulmonal_concept.concept_id and gejala_tb_ekstrapulmonal_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_status_hiv ON scr_int_enc.encounter_id = obs_status_hiv.encounter_id and obs_status_hiv.concept_id = 166909 and obs_status_hiv.voided = 0
LEFT JOIN hydra.concept_name status_hiv_concept ON  obs_status_hiv.value_coded = status_hiv_concept.concept_id   and status_hiv_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'TST Test Details'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) tst_test_enc ON enc.patient_id = tst_test_enc.patient_id
LEFT JOIN hydra.obs obs_kesimpulan_pemeriksaan_uji_tuberculin ON tst_test_enc.encounter_id = obs_kesimpulan_pemeriksaan_uji_tuberculin.encounter_id and obs_kesimpulan_pemeriksaan_uji_tuberculin.concept_id = 166963 and obs_kesimpulan_pemeriksaan_uji_tuberculin.voided = 0
LEFT JOIN hydra.concept_name kesimpulan_pemeriksaan_uji_tuberculin_concept ON  obs_kesimpulan_pemeriksaan_uji_tuberculin.value_coded = kesimpulan_pemeriksaan_uji_tuberculin_concept.concept_id  and kesimpulan_pemeriksaan_uji_tuberculin_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Chest X-ray'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) chest_xray_enc ON enc.patient_id = chest_xray_enc.patient_id
LEFT JOIN hydra.obs obs_hasil_pembacaan_foto_rontgen ON tst_test_enc.encounter_id = obs_hasil_pembacaan_foto_rontgen.encounter_id and obs_hasil_pembacaan_foto_rontgen.concept_id = 166948 and obs_hasil_pembacaan_foto_rontgen.voided = 0
LEFT JOIN hydra.concept_name hasil_pembacaan_foto_rontgen_concept ON  obs_hasil_pembacaan_foto_rontgen.value_coded = hasil_pembacaan_foto_rontgen_concept.concept_id and hasil_pembacaan_foto_rontgen_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_kelainan_pada_paru_atau_pleura ON tst_test_enc.encounter_id = obs_kelainan_pada_paru_atau_pleura.encounter_id and obs_kelainan_pada_paru_atau_pleura.concept_id = 166945 and obs_kelainan_pada_paru_atau_pleura.voided = 0
LEFT JOIN hydra.concept_name kelainan_pada_paru_atau_pleura_concept ON  obs_kelainan_pada_paru_atau_pleura.value_coded = kelainan_pada_paru_atau_pleura_concept.concept_id  and kelainan_pada_paru_atau_pleura_concept.locale_preferred = 1
left JOIN hydra.encounter lat_enc ON lat_enc.patient_id = per.person_id and lat_enc.voided = 0
left join hydra.hydramodule_form_encounter hm_form_enc on hm_form_enc.encounter_id = lat_enc.encounter_id
left join hydra.hydramodule_component_form hm_comp_frm on hm_form_enc.component_form_id = hm_comp_frm.component_form_id
left join hydra.hydramodule_workflow hm_wf on hm_comp_frm.workflow_id = hm_wf.workflow_id
where
   lat_enc.encounter_id = (
      SELECT enc.encounter_id
      FROM hydra.encounter enc
      WHERE enc.patient_id = per.person_id
      order by encounter_datetime desc limit 1
   ) and pt.voided = 0 
and mengumpulkan_dahak_concept.name = 'Ya' and (hasil_xpert_mtb_concept.name is null or hasil_xpert_mtb_concept.name = 'Error' or hasil_xpert_mtb_concept.name = 'Invalid' or hasil_xpert_mtb_concept.name = 'No Result')
and (find_in_set(hlt_ctr_name.name, $P{facility}) or $P{facility} is NULL)
and date(enc.max_enc_datetime) between date($P{from}) and date($P{to});]]>
	</queryString>
