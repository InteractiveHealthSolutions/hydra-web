	<queryString>
		<![CDATA[select pt_id.identifier as patient_id, ex_id.identifier as patient_external_id, loc.name as patient_location,
per_name.given_name as patient_first_name, per_name.family_name as patient_last_name, per.gender as patient_gender,
TIMESTAMPDIFF(YEAR, per.birthdate, CURDATE()) AS patient_age, pri_cont.value as primary_contact,
sec_cont.value as secondary_contact, hlt_ctr_name.name  as location_name,
per_add.state_province as province, per_add.city_village as city,
enc_tb_diagnosis.encounter_datetime as tb_confirmation_datetime, loc_tb_diagnosis.name as loc_tb_diagnosis_location,
tbtype_concept.name as tb_type, mtbconfirmation_concept.name as mtb_confirmation, confirmeddiagnosis_concept.name as confirmed_diagnosis,
drug_resistance_profile_concept.name as drug_resistance_profile, eligible_for_treatment_concept.name as eligible_for_treatment,
IF(drug_resistance_profile_concept.name = 'confirmed_drug_susceptible' and eligible_for_treatment_concept.name = 'yes','Yes','No') as need_treatment_initiation,
enc_adlt_treatment_initiation.max_enc_datetime as adlt_treatment_initiation_datetime, loc_adlt_treatment_initiation.name as loc_adlt_treatment_initiation,
obs_tb_reg_no.value_text as tb_reg_no, treatment_initiation_concept.name as treatment_initiated, reason_treatment_notinitiated_concept.name as reason_treatment_notinitiated,
sickness_duration_concept.name as duration_of_sickness_prior_to_tb_diagnosis, patient_type_concept.name as patient_type, tb_cat_concept.name as tb_cat, treatment_plan_concept.name as treatment_plan_initiation,
enc_adlt_treatment_followup.max_enc_datetime as adlt_treatment_followup_datetime, loc_adlt_treatment_followup.name as loc_adlt_treatment_followup,
obs_treatment_month.value_numeric as treatment_month, treatment_plan_fp_concept.name as treatment_plan_fp, patient_clinical_status_concept.name as patient_clinical_status,
obs_followup_needed_concept.name as followup_needed, obs_next_appt_date.value_datetime as next_apt_date,
IF(DATEDIFF(CURDATE(), obs_next_appt_date.value_datetime) < 3,'No',IF(enc_eof.max_enc_datetime >= enc_adlt_treatment_initiation.max_enc_datetime,'No',IF(enc_eof.max_enc_datetime >= enc_adlt_treatment_initiation.max_enc_datetime,'No', 'Yes'))) as missed_followup,
enc_eof.max_enc_datetime as end_of_followup_datetime, loc_eof.name as loc_end_of_followup, tx_outcome_concept.name as tx_outcome,
obs_remarks.value_text as remarks
FROM ( select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'TB Disease Confirmation'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) AS encs
INNER JOIN hydra.encounter AS enc_tb_diagnosis
    ON enc_tb_diagnosis.patient_id = encs.patient_id
   AND enc_tb_diagnosis.encounter_datetime = encs.max_enc_datetime
   AND enc_tb_diagnosis.encounter_type = encs.encounter_type_id
inner join hydra.patient pt on pt.patient_id = enc_tb_diagnosis.patient_id
inner join hydra.patient_identifier pt_id on pt.patient_id = pt_id.patient_id and pt_id.identifier_type = 3 and pt_id.voided = 0
left join hydra.patient_identifier ex_id on pt.patient_id = ex_id.patient_id and ex_id.identifier_type = 5 and ex_id.voided = 0
left join hydra.location loc on pt_id.location_id = loc.location_id
left join hydra.person per on pt.patient_id = per.person_id
left join hydra.person_name per_name on per.person_id = per_name.person_id and per_name.voided = 0 and per_name.preferred = 1
left join hydra.person_attribute pri_cont on per.person_id = pri_cont.person_id and pri_cont.person_attribute_type_id = 8 and pri_cont.voided = 0
left join hydra.person_attribute sec_cont on per.person_id = sec_cont.person_id and sec_cont.person_attribute_type_id = 12 and sec_cont.voided = 0
left join hydra.person_attribute hlt_ctr on per.person_id = hlt_ctr.person_id and hlt_ctr.person_attribute_type_id = 7 and sec_cont.voided = 0
left join hydra.location hlt_ctr_name on hlt_ctr.value = hlt_ctr_name.location_id
left join hydra.person_address per_add on per.person_id = per_add.person_id and per_add.voided = 0 and per_add.preferred = 1
LEFT JOIN hydra.location loc_tb_diagnosis ON enc_tb_diagnosis.location_id = loc_tb_diagnosis.location_id
LEFT JOIN hydra.obs obs_tbtype ON enc_tb_diagnosis.encounter_id = obs_tbtype.encounter_id and obs_tbtype.concept_id = 160040 and obs_tbtype.voided = 0
LEFT JOIN hydra.concept_name tbtype_concept ON  obs_tbtype.value_coded = tbtype_concept.concept_id and tbtype_concept.concept_name_type = 'SHORT'  and tbtype_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_mtbconfirmation ON enc_tb_diagnosis.encounter_id = obs_mtbconfirmation.encounter_id and obs_mtbconfirmation.concept_id = 164998 and obs_mtbconfirmation.voided = 0
LEFT JOIN hydra.concept_name mtbconfirmation_concept ON  obs_mtbconfirmation.value_coded = mtbconfirmation_concept.concept_id and mtbconfirmation_concept.concept_name_type = 'SHORT'  and mtbconfirmation_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_confirmeddiagnosis ON enc_tb_diagnosis.encounter_id = obs_confirmeddiagnosis.encounter_id and obs_confirmeddiagnosis.concept_id = 159392 and obs_confirmeddiagnosis.voided = 0
LEFT JOIN hydra.concept_name confirmeddiagnosis_concept ON  obs_confirmeddiagnosis.value_coded = confirmeddiagnosis_concept.concept_id and confirmeddiagnosis_concept.concept_name_type = 'SHORT'  and confirmeddiagnosis_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_drug_resistance_profile ON enc_tb_diagnosis.encounter_id = obs_drug_resistance_profile.encounter_id and obs_drug_resistance_profile.concept_id = 165029 and obs_drug_resistance_profile.voided = 0
LEFT JOIN hydra.concept_name drug_resistance_profile_concept ON  obs_drug_resistance_profile.value_coded = drug_resistance_profile_concept.concept_id and drug_resistance_profile_concept.concept_name_type = 'SHORT'  and drug_resistance_profile_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_eligible_for_treatment ON enc_tb_diagnosis.encounter_id = obs_eligible_for_treatment.encounter_id and obs_eligible_for_treatment.concept_id = 166032 and obs_eligible_for_treatment.voided = 0
LEFT JOIN hydra.concept_name eligible_for_treatment_concept ON  obs_eligible_for_treatment.value_coded = eligible_for_treatment_concept.concept_id and eligible_for_treatment_concept.concept_name_type = 'SHORT'  and eligible_for_treatment_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Adult TB Treatment Initiation Form'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) enc_adlt_treatment_initiation ON enc_adlt_treatment_initiation.patient_id = enc_tb_diagnosis.patient_id
LEFT JOIN hydra.location loc_adlt_treatment_initiation ON enc_adlt_treatment_initiation.location_id = loc_adlt_treatment_initiation.location_id
LEFT JOIN hydra.obs obs_tb_reg_no ON enc_adlt_treatment_initiation.encounter_id = obs_tb_reg_no.encounter_id and obs_tb_reg_no.concept_id = 164995 and obs_tb_reg_no.voided = 0
LEFT JOIN hydra.obs obs_treatment_initiation ON enc_adlt_treatment_initiation.encounter_id = obs_treatment_initiation.encounter_id and obs_treatment_initiation.concept_id = 164647 and obs_treatment_initiation.voided = 0
LEFT JOIN hydra.concept_name treatment_initiation_concept ON  obs_treatment_initiation.value_coded = treatment_initiation_concept.concept_id and treatment_initiation_concept.concept_name_type = 'SHORT'  and treatment_initiation_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_reason_treatment_notinitiated ON enc_adlt_treatment_initiation.encounter_id = obs_reason_treatment_notinitiated.encounter_id and obs_reason_treatment_notinitiated.concept_id = 164437 and obs_reason_treatment_notinitiated.voided = 0
LEFT JOIN hydra.concept_name reason_treatment_notinitiated_concept ON  obs_reason_treatment_notinitiated.value_coded = reason_treatment_notinitiated_concept.concept_id and reason_treatment_notinitiated_concept.concept_name_type = 'SHORT'  and reason_treatment_notinitiated_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_sickness_duration ON enc_adlt_treatment_initiation.encounter_id = obs_sickness_duration.encounter_id and obs_sickness_duration.concept_id = 166803 and obs_sickness_duration.voided = 0
LEFT JOIN hydra.concept_name sickness_duration_concept ON  obs_sickness_duration.value_coded = sickness_duration_concept.concept_id and sickness_duration_concept.concept_name_type = 'SHORT'  and sickness_duration_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_patient_type ON enc_adlt_treatment_initiation.encounter_id = obs_patient_type.encounter_id and obs_patient_type.concept_id = 159990 and obs_patient_type.voided = 0
LEFT JOIN hydra.concept_name patient_type_concept ON  obs_patient_type.value_coded = patient_type_concept.concept_id and patient_type_concept.concept_name_type = 'SHORT'  and patient_type_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_tb_cat ON enc_adlt_treatment_initiation.encounter_id = obs_tb_cat.encounter_id and obs_tb_cat.concept_id = 164238 and obs_tb_cat.voided = 0
LEFT JOIN hydra.concept_name tb_cat_concept ON  obs_tb_cat.value_coded = tb_cat_concept.concept_id and tb_cat_concept.concept_name_type = 'SHORT'  and tb_cat_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_treatment_plan ON enc_adlt_treatment_initiation.encounter_id = obs_treatment_plan.encounter_id and obs_treatment_plan.concept_id = 1268 and obs_treatment_plan.voided = 0
LEFT JOIN hydra.concept_name treatment_plan_concept ON  obs_treatment_plan.value_coded = treatment_plan_concept.concept_id and treatment_plan_concept.concept_name_type = 'SHORT'  and treatment_plan_concept.locale_preferred = 1
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Adult TB Treatment Followup Form'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) enc_adlt_treatment_followup ON enc_adlt_treatment_followup.patient_id = enc_tb_diagnosis.patient_id
LEFT JOIN hydra.location loc_adlt_treatment_followup ON enc_adlt_treatment_followup.location_id = loc_adlt_treatment_followup.location_id
LEFT JOIN hydra.obs obs_treatment_month ON enc_adlt_treatment_followup.encounter_id = obs_treatment_month.encounter_id and obs_treatment_month.concept_id = 164327 and obs_treatment_month.voided = 0
LEFT JOIN hydra.obs obs_treatment_plan_fp ON enc_adlt_treatment_followup.encounter_id = obs_treatment_plan_fp.encounter_id and obs_treatment_plan_fp.concept_id = 1268 and obs_treatment_plan_fp.voided = 0
LEFT JOIN hydra.concept_name treatment_plan_fp_concept ON  obs_treatment_plan_fp.value_coded = treatment_plan_fp_concept.concept_id and treatment_plan_fp_concept.concept_name_type = 'SHORT'  and treatment_plan_fp_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_patient_clinical_status ON enc_adlt_treatment_followup.encounter_id = obs_patient_clinical_status.encounter_id and obs_patient_clinical_status.concept_id = 166061 and obs_patient_clinical_status.voided = 0
LEFT JOIN hydra.concept_name patient_clinical_status_concept ON  obs_patient_clinical_status.value_coded = patient_clinical_status_concept.concept_id and patient_clinical_status_concept.concept_name_type = 'SHORT'  and patient_clinical_status_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_followup_needed ON enc_adlt_treatment_followup.encounter_id = obs_followup_needed.encounter_id and obs_followup_needed.concept_id = 166291 and obs_followup_needed.voided = 0
LEFT JOIN hydra.concept_name obs_followup_needed_concept ON  obs_followup_needed.value_coded = obs_followup_needed_concept.concept_id and obs_followup_needed_concept.concept_name_type = 'SHORT'  and obs_followup_needed_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_next_appt_date ON enc_adlt_treatment_followup.encounter_id = obs_next_appt_date.encounter_id and obs_next_appt_date.concept_id = 5096 and obs_next_appt_date.voided = 0
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_type, location_id, encounter_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'End of Followup Form'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_type, location_id, encounter_id) enc_eof ON enc_eof.patient_id = enc_tb_diagnosis.patient_id
LEFT JOIN hydra.location loc_eof ON enc_eof.location_id = loc_eof.location_id
LEFT JOIN hydra.obs obs_tx_outcome ON enc_eof.encounter_id = obs_tx_outcome.encounter_id and obs_tx_outcome.concept_id = 159786 and obs_tx_outcome.voided = 0
LEFT JOIN hydra.concept_name tx_outcome_concept ON  obs_tx_outcome.value_coded = tx_outcome_concept.concept_id and tx_outcome_concept.concept_name_type = 'SHORT'  and tx_outcome_concept.locale_preferred = 1
LEFT JOIN hydra.obs obs_remarks ON enc_eof.encounter_id = obs_remarks.encounter_id and obs_remarks.concept_id = 16811 and obs_remarks.voided = 0
LEFT JOIN (select patient_id , MAX(encounter_datetime) AS max_enc_datetime, encounter_id, location_id from hydra.encounter
			INNER JOIN hydra.encounter_type enc_type ON encounter.encounter_type = enc_type.encounter_type_id AND enc_type.name = 'Adult TB Screening Form'
			WHERE encounter.voided = 0
			GROUP BY encounter.patient_id, encounter_id) enc_adlt_screening ON enc_adlt_screening.patient_id = enc_tb_diagnosis.patient_id
LEFT JOIN hydra.obs obs_adlt_scr_presumptivetb ON enc_adlt_screening.encounter_id = obs_adlt_scr_presumptivetb.encounter_id and obs_adlt_scr_presumptivetb.concept_id = 166778 and obs_adlt_scr_presumptivetb.voided = 0
LEFT JOIN hydra.concept_name presumptivetb_concept ON obs_adlt_scr_presumptivetb.value_coded = obs_adlt_scr_presumptivetb.concept_id and presumptivetb_concept.concept_name_type = 'SHORT'  and presumptivetb_concept.locale_preferred = 1
left JOIN hydra.encounter lat_enc ON lat_enc.patient_id = per.person_id and lat_enc.voided = 0
left join hydra.hydramodule_form_encounter hm_form_enc on hm_form_enc.encounter_id = lat_enc.encounter_id
left join hydra.hydramodule_component_form hm_comp_frm on hm_form_enc.component_form_id = hm_comp_frm.component_form_id
left join hydra.hydramodule_workflow hm_wf on hm_comp_frm.workflow_id = hm_wf.workflow_id
where
   lat_enc.encounter_id = (
      SELECT enc.encounter_id
      FROM hydra.encounter enc
      WHERE enc.patient_id = per.person_id
      order by encounter_datetime desc limit 1
   )
and enc_tb_diagnosis.voided= 0 and pt.voided = 0 and (find_in_set(hlt_ctr_name.name, $P{facility}) or $P{facility} is NULL)
and date(enc_tb_diagnosis.encounter_datetime) between date($P{from}) and date($P{to})
and (hm_wf.name = $P{workflow} or $P{workflow} is NULL)
and (presumptivetb_concept.name = $P{presumptive_tb} or $P{presumptive_tb} is NULL);]]>
	</queryString>
